#include <iostream> //добавление библиотеки ввода/вывода
#include <math.h> //добавление библиотеки математических функций

using namespace std;

float fact2(int n) {
    int temp = 1;
    for (; n > 0; n -= 2) temp *= n;
    return temp;
}

int main(void)
{
    int i, n;
    for (i = 1; i <= 5; ++i) {
        cout << "N:";
        cin >> n;
        cout << "fact: " << fact2(n) << endl << endl;
    }
    return 0;
}
